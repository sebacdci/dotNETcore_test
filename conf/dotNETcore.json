{
  "id": "dotnet",
  "cmd": null,
  "cpus": 0.2,
  "mem": 256,
  "container": {
    "type": "DOCKER",
    "docker": {
      "image": "microsoft/dotnet-samples:aspnetapp",
      "network": "BRIDGE"
    },
    "portMappings": [
      { "containerPort": 80, "hostPort": 0 }
    ]
  },
  "labels": 
     {
     "DCOS_SERVICE_NAME": "aspnetapp",
     "DCOS_SERVICE_SCHEME": "http",
     "DCOS_SERVICE_PORT_INDEX": "0"
     },
  "healthChecks": [
  {
    "gracePeriodSeconds": 300,
    "intervalSeconds": 60,
    "timeoutSeconds": 20,
    "maxConsecutiveFailures": 3,
    "portIndex": 0,
    "path": "/",
    "protocol": "HTTP",
    "ignoreHttp1xx": false
  }
]
    
}
  
  
